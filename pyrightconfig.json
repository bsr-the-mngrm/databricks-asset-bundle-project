{
  "include": [
    "src",
    "tests"
  ],
  "extraPaths": [
    "src",
    "/usr/local/lib/python3.12/site-packages"
  ],
  "exclude": [
    "resources/notebooks/*.py"
  ],
  "ignore": [
    "resources/notebooks/*.py"
  ],
  "typeCheckingMode": "strict",
  "pythonVersion": "3.12",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false
}